{
    "breadcrumbs.enabled": false,
    "debug.console.fontFamily": "'Consolas', monospace",
    "debug.console.fontSize": 12,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.useInlineViewWhenSpaceIsLimited": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "'Consolas', monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 12,
    "editor.inlayHints.fontSize": 10.5,
    "editor.inlayHints.padding": true,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80, 120
    ],
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "explorer.decorations.badges": false,
    "explorer.sortOrder": "type",
    "extensions.autoUpdate": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.untrackedChanges": "separate",
    "problems.decorations.enabled": false,
    "problems.showCurrentInStatus": true,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontFamily": "'Consolas', monospace",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "window.commandCenter": false,
    "window.menuBarVisibility": "classic",
    "window.title": "${rootName}${separator}${dirty}${activeEditorShort}${separator}${appName}",
    "window.titleSeparator": " â€¢ ",
    "workbench.colorCustomizations": {
        "tab.activeForeground": "#000",
        "tab.inactiveBackground": "#ddd"
    },
    "workbench.colorTheme": "Default Light+",
    "workbench.editor.closeOnFileDelete": false,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.empty.hint": "hidden",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
}
